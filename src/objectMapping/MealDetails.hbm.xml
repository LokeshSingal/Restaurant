<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="model.MealDetails" table="MealDetails">
      <id name="mealDetailsId" type="int" column="mealDetailsId">
         <generator class="increment" />
      </id>
      <property name="mealId" column="mealId" type="int"/>
      <property name="itemId" column="itemId" type="int"/>
      <property name="price" column="price" type="float"/>
      <property name="qty" column="qty" type="int"/>
   </class>
</hibernate-mapping>